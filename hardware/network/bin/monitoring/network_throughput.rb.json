{
  "name": "Network Throughput",
  "key": "net",
  "metrics": {
    "tx.bytes": {
      "desc": "Outgoing bytes/sec",
      "unit": "Bps"
    },
    "tx.packets": {
      "desc": "Outgoing packets/sec",
      "unit": "pkts/s"
    },
    "rx.bytes": {
      "desc": "Incoming bytes/sec",
      "unit": "Bps"
    },
    "rx.packets": {
      "desc": "Incoming packets/sec",
      "unit": "pkts/s"
    }
  },

  "tags" : [ "interface" ],

  "options": {
    "interfaces": {
      "name": "Interfaces",
      "desc": "Only interfaces that match the given regular expression will be monitored",
      "default": "venet|eth|en",
      "type": "text"
    }
  }
}
